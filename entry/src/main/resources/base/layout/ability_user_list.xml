<?xml version="1.0" encoding="utf-8"?>
<DependentLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:height="120vp"
    ohos:width="match_parent"
    ohos:background_element="$graphic:inpage_button_element_selector"
    ohos:top_margin="15vp"
    ohos:top_padding="15vp">
    <DirectionalLayout
        ohos:height="match_content"
        ohos:width="match_parent">
        <Text
            ohos:id="$+id:name_tv"
            ohos:height="match_content"
            ohos:width="match_parent"
            ohos:left_margin="18vp"
            ohos:max_text_lines="2"
            ohos:multiple_lines="true"
            ohos:right_margin="8vp"
            ohos:text="用户名"
            ohos:text_color="#2E2E2E"
            ohos:text_size="24fp"
            />

        <Text
            ohos:id="$+id:commrate_tv"
            ohos:height="match_content"
            ohos:width="match_content"
            ohos:left_margin="18vp"
            ohos:text="昵称"
            ohos:top_margin="30vp"
            ohos:text_color="#FF000000"
            ohos:text_size="24fp"/>
    </DirectionalLayout>

    <DirectionalLayout
        ohos:height="match_content"
        ohos:width="match_parent">
        <Text
            ohos:id="$+id:user_list_page_userName_id"
            ohos:height="match_content"
            ohos:width="match_parent"
            ohos:left_margin="200vp"
            ohos:multiple_lines="true"
            ohos:right_margin="8vp"
            ohos:text="hsd"
            ohos:text_color="#2E2E2E"
            ohos:text_size="20fp"
            />
        <Text
            ohos:id="$+id:user_list_page_nickName_id"
            ohos:height="match_content"
            ohos:width="match_content"
            ohos:left_margin="200vp"
            ohos:text="黄帅迪"
            ohos:top_margin="35vp"
            ohos:text_color="#FF000000"
            ohos:text_size="20fp"/>
    </DirectionalLayout>




</DependentLayout>